<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->

        <item name="colorPrimary">#660000</item>
        <item name="colorPrimaryDark">#660000</item>
        <item name="colorAccent">#000000</item>
        <item name="android:statusBarColor">@color/maroon</item>
    </style>
    <style name="coba" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->

        <item name="colorPrimary">#660000</item>
        <item name="colorPrimaryDark">#660000</item>
        <item name="colorAccent">#000000</item>
    </style>
    <style name="AppTheme.NoActionBar" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="windowActionBar">true</item>
        <item name="windowNoTitle">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:navigationBarColor">@color/maroon</item>
        <item name="android:statusBarColor">@color/maroon</item>
    </style>
    <style name="ini" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="AppTheme.NoActionBar.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.NoActionBar.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


</resources>
